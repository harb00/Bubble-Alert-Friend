<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>

		<GenerateDependencyFile>false</GenerateDependencyFile><!-- deps.json file -->
		<ProduceReferenceAssembly>false</ProduceReferenceAssembly> <!-- ref folder -->
		<DebugType>None</DebugType> <!-- .pdb file -->

		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<Reference Include="Plus Emulator">
			<HintPath>$(PLUS_EMULATOR_HOME)\Plus Emulator.dll</HintPath>
			<Private>false</Private>
			<ExcludeAssets>all</ExcludeAssets>
		</Reference>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
	</ItemGroup>
</Project>
